# BOB Google Maps V3.0 - Environment Configuration
# Author: Divyanshu Singh Chouhan
# Release: October 3, 2025

# Extractor Settings
BOB_HEADLESS=true
BOB_TIMEOUT=60
BOB_MAX_RETRIES=3
BOB_STEALTH=true
BOB_INTERCEPT=true
BOB_BLOCK_RESOURCES=true
BOB_MAX_REVIEWS=10
BOB_MAX_IMAGES=20

# Cache Settings
BOB_CACHE_ENABLED=true
BOB_CACHE_PATH=bob_cache_ultimate.db
BOB_CACHE_HOURS=24
BOB_AUTO_CLEANUP=true
BOB_CLEANUP_DAYS=7

# Parallel Processing
BOB_PARALLEL_ENABLED=true
BOB_MAX_CONCURRENT=10
BOB_CONTEXT_POOL=5
BOB_MAX_MEMORY_MB=2048
BOB_BATCH_SIZE=100

# Logging
BOB_LOG_LEVEL=INFO
BOB_LOG_FILE=logs/bob.log

# Paths
BOB_DATA_DIR=./data
BOB_CACHE_DIR=./cache
BOB_LOGS_DIR=./logs
